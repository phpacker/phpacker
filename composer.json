{
    "name": "phpacker/phpacker",
    "description": "Package a PHP script as a standalone cross-platform binary. Convenience wrapper around static-php & phpmicro using general-purpose php binaries",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "files": [
            "src/bootstrap.php",
            "src/constants.php"
        ],
        "psr-4": {
            "PHPacker\\PHPacker\\": "src/",
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "gwleuverink",
            "email": "willem@leuver.ink"
        }
    ],
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpacker/composer-installer": true
        }
    },
    "extra": {
        "phpacker-install": "phpacker",
        "thanks": [
            {
                "name": "Static PHP CLI",
                "url": "https://github.com/crazywhalecc/static-php-cli"
            },
            {
                "name": "phpmicro",
                "url": "https://github.com/easysoft/phpmicro"
            },
            {
                "name": "NativePHP",
                "url": "https://github.com/nativephp/php-bin"
            }
        ]
    },
    "require": {
        "laravel/prompts": "^0.3",
        "spatie/once": "^3.1",
        "symfony/console": "^7.2",
        "symfony/filesystem": "^7.2",
        "symfony/finder": "^7.2",
        "symfony/event-dispatcher": "^7.2",
        "phpacker/composer-installer": "^0.3"
    },
    "require-dev": {
        "laravel/pint": "^1.20",
        "nunomaduro/collision": "^8.6",
        "phpstan/phpstan": "^2.1",
        "pestphp/pest": "^3.7",
        "symfony/process": "^7.2",
        "cpx/cpx": "^1.0"
    },
    "scripts": {
        "test": "./vendor/bin/pest",
        "format": "./vendor/bin/pint",
        "analyze": "./vendor/bin/phpstan",
        "build": [
            "cpx box compile",
            "bin/phpacker build"
        ]
    },
    "minimum-stability": "stable"
}
